package br.com.entra21.modelo2022.backend.area.logada.cadastros.crud;

import java.util.ArrayList;
import java.util.Arrays;

public interface ICrud {

	
	ArrayList<String> opcoes=new ArrayList<String>(Arrays.asList("Listar","Adicionar", "Buscar","Editar","Deletar"));
	public void listar();
	public void adicionar();
	public void buscar();
	public void editar();
	public void exluir();
	public Object capturarChave();
	public Object capturarValores();

}
